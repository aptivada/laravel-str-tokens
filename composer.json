{
    "name": "fomvasss/laravel-str-tokens",
    "description": "A package to manage and generate string with tokens/shortcodes for Eloquent Models",
    "type": "composer-package",
    "keywords": ["token", "tag", "string", "shortcodes", "laravel", "drupal"],
    "homepage": "https://github.com/fomvasss/laravel-str-tokens",
    "authors": [
        {
            "name": "Fomin Vasyl",
            "email": "fomvasss@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php" : ">=7.0.0",
        "illuminate/support": "^10.0|^11.0",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Fomvasss\\LaravelStrTokens\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fomvasss\\LaravelStrTokens\\ServiceProvider"
            ],
            "aliases": {
                "StrToken": "Fomvasss\\LaravelStrTokens\\Facades\\StrToken"
            }
        }
    }
}
